<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">

	<!-- define the target user to be a context parameter,which will be replaced while package by maven -->
	<context-param>
		<param-name>target.user</param-name>
		<param-value>${target.user}</param-value>
	</context-param>

	<listener>
		<listener-class>com.sean.service.core.ServletContextListener</listener-class>
	</listener>
	
</web-app>